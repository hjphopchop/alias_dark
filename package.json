{
  "name": "alias-dark",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run docker:up && npm run build:types && dotenv -c -- next dev",
    "build": "next build",
    "build:apollo": "mkdirp .generated/graphql && apollo client:codegen -c apollo.config.js --outputFlat --target=typescript .generated/graphql",
    "build:nexus": "ts-node --transpile-only -r tsconfig-paths/register src/graphql/schema.ts",
    "build:prisma": "dotenv -c -- prisma generate DISABLE_ERD=true",
    "build:types": "npm run -s build:prisma && npm run -s build:nexus && npm run -s build:apollo",
    "start": "next start",
    "db:migrate:dev": "dotenv -c -- prisma migrate dev",
    "db:studio": "dotenv -e .env.local -- npx prisma studio -p 3322",
    "docker:down": "docker compose down",
    "docker:up": "docker compose up -d",
    "lint": "npm run -s lint:eslint && npm run -s lint:prettier && npm run -s lint:ts",
    "lint:eslint": "eslint --ext=.js,.jsx,.mjs,.ts,.tsx --ignore-path .gitignore",
    "lint:prettier": "prettier --write ./**/*.{js,jsx,ts,tsx,css,md,json} --config .prettierrc",
    "lint:ts": "tsc",
    "pre-commit": "lint-staged && npm run -s lint:ts",
    "prepare": "husky install",
    "codegen": "apollo client:codegen --target typescript",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "e2e": "start-server-and-test dev http://localhost:3333 \"cypress open --e2e\"",
    "e2e:headless": "start-server-and-test dev http://localhost:3001 \"cypress run --e2e\"",
    "component": "cypress open --component",
    "component:headless": "cypress run --component"
  },
  "lint-staged": {
    "*.json": "npm run -s lint:prettier -- --check",
    "src/**/*.@(jsx|js|ts|tsx)": [
      "npm run -s lint:prettier -- --check",
      "npm run -s lint:eslint"
    ],
    "*.css": "npm run -s lint:prettier -- --check",
    "*.@(yaml|yml)": "npm run -s lint:prettier -- --check"
  },
  "dependencies": {
    "@apollo/client": "^3.7.4",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@next/env": "^13.1.2",
    "@next/font": "13.1.2",
    "@nexus/schema": "^0.20.1",
    "@prisma/client": "^4.10.1",
    "@types/node": "^12.0.2",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "apollo": "^2.34.0",
    "apollo-server-core": "^3.11.1",
    "apollo-server-micro": "^3.11.1",
    "autoprefixer": "^10.4.13",
    "axios": "^1.3.5",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^6.0.0",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.2",
    "framer-motion": "^9.0.7",
    "framer-motion-3d": "^9.0.7",
    "fs": "^0.0.1-security",
    "graphql": "15.7.2",
    "micro": "^10.0.1",
    "micro-cors": "^0.1.1",
    "multer": "^1.4.5-lts.1",
    "next": "13.1.2",
    "next-auth": "^4.19.2",
    "next-connect": "^0.13.0",
    "nexus": "^1.3.0",
    "nexus-prisma": "^1.0.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "prisma": "^4.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.3",
    "tailwindcss": "^3.2.4",
    "tsconfig-paths": "^4.1.2",
    "typescript": "4.9.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/micro-cors": "^0.1.2",
    "@types/multer": "^1.4.7",
    "@types/uuid": "^9.0.1",
    "cypress": "^12.6.0",
    "dotenv": "^16.0.3",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.0",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "start-server-and-test": "^1.15.4",
    "ts-node": "^10.9.1"
  },
  "engines": {
    "node": "16.x"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "resolutions": {
    "graphql": "15.7.2",
    "@types/react": "^18.0.15"
  }
}
