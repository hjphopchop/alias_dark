{
  "name": "alias-dark",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run docker:up && npm run build:types && dotenv -c -- next dev",
    "build": "next build",
    "build:apollo": "mkdirp .generated/graphql && apollo client:codegen -c apollo.config.js --outputFlat --target=typescript .generated/graphql",
    "build:nexus": "ts-node --transpile-only -r tsconfig-paths/register src/graphql/schema.ts",
    "build:prisma": "dotenv -c -- prisma generate DISABLE_ERD=true",
    "build:types": "npm run -s build:prisma && npm run -s build:nexus && npm run -s build:apollo",
    "start": "next start",
    "db:migrate:dev": "dotenv -c -- prisma migrate dev",
    "docker:down": "docker compose down",
    "docker:up": "docker compose up -d",
    "lint": "npm run -s lint:eslint && npm run -s lint:prettier && npm run -s lint:ts",
    "lint:eslint": "eslint --ext=.js,.jsx,.mjs,.ts,.tsx --ignore-path .gitignore",
    "lint:prettier": "prettier --write ./**/*.{js,jsx,ts,tsx,css,md,json} --config .prettierrc",
    "lint:ts": "tsc",
    "pre-commit": "lint-staged && npm run -s lint:ts",
    "prepare": "husky install",
    "codegen": "apollo client:codegen --target typescript"
  },
  "lint-staged": {
    "*.json": "npm run -s lint:prettier -- --check",
    "src/**/*.@(jsx|js|ts|tsx)": [
      "npm run -s lint:prettier -- --check",
      "npm run -s lint:eslint"
    ],
    "*.css": "npm run -s lint:prettier -- --check",
    "*.@(yaml|yml)": "npm run -s lint:prettier -- --check"
  },
  "dependencies": {
    "@apollo/client": "^3.7.4",
    "@next/env": "^13.1.2",
    "@next/font": "13.1.2",
    "@nexus/schema": "^0.20.1",
    "@prisma/client": "^4.8.1",
    "@types/node": "^12.0.2",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "apollo": "^2.34.0",
    "apollo-server-core": "^3.11.1",
    "apollo-server-micro": "^3.11.1",
    "axios": "^1.2.2",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^6.0.0",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.2",
    "graphql": "15.7.2",
    "micro": "^10.0.1",
    "micro-cors": "^0.1.1",
    "next": "13.1.2",
    "next-auth": "^4.18.8",
    "nexus": "^1.3.0",
    "nexus-prisma": "^1.0.3",
    "prettier": "^2.8.3",
    "prisma": "^4.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.2.4",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/micro-cors": "^0.1.2",
    "dotenv": "^16.0.3",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.0",
    "prettier-plugin-tailwindcss": "^0.2.1"
  },
  "engines": {
    "node": ">=16.4.0"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "resolutions": {
    "graphql": "15.7.2",
    "@types/react": "^18.0.15"
  }
}
